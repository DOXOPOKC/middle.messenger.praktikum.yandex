{
    "compilerOptions": {
        "downlevelIteration": true,
        "noImplicitAny": true,
        "sourceMap": true,
        "strictNullChecks": true,
        "target": "ESNext",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
    },
    "include": ["src/**/*"]
}
